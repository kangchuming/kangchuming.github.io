#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/new/Documents/code/kangchuming.github.io/node_modules/.pnpm/vitepress@1.0.2_@algolia+client-search@4.23.2_postcss@8.4.38_search-insights@2.13.0/node_modules/vitepress/bin/node_modules:/Users/new/Documents/code/kangchuming.github.io/node_modules/.pnpm/vitepress@1.0.2_@algolia+client-search@4.23.2_postcss@8.4.38_search-insights@2.13.0/node_modules/vitepress/node_modules:/Users/new/Documents/code/kangchuming.github.io/node_modules/.pnpm/vitepress@1.0.2_@algolia+client-search@4.23.2_postcss@8.4.38_search-insights@2.13.0/node_modules:/Users/new/Documents/code/kangchuming.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/new/Documents/code/kangchuming.github.io/node_modules/.pnpm/vitepress@1.0.2_@algolia+client-search@4.23.2_postcss@8.4.38_search-insights@2.13.0/node_modules/vitepress/bin/node_modules:/Users/new/Documents/code/kangchuming.github.io/node_modules/.pnpm/vitepress@1.0.2_@algolia+client-search@4.23.2_postcss@8.4.38_search-insights@2.13.0/node_modules/vitepress/node_modules:/Users/new/Documents/code/kangchuming.github.io/node_modules/.pnpm/vitepress@1.0.2_@algolia+client-search@4.23.2_postcss@8.4.38_search-insights@2.13.0/node_modules:/Users/new/Documents/code/kangchuming.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
